{
  "name": "frame-agent-sdk",
  "version": "0.0.1",
  "description": "Framework criador de agentes com motor baseado em grafos, módulos reutilizáveis e um orchestrator baseado em steps muito simples e fácil de configurar.",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./agents": {
      "import": "./dist/src/agents/index.js",
      "require": "./dist/src/agents/index.js",
      "types": "./dist/src/agents/index.d.ts"
    },
    "./llm": {
      "import": "./dist/src/llm/index.js",
      "require": "./dist/src/llm/index.js",
      "types": "./dist/src/llm/index.d.ts"
    },
    "./memory": {
      "import": "./dist/src/memory/index.js",
      "require": "./dist/src/memory/index.js",
      "types": "./dist/src/memory/index.d.ts"
    },
    "./orchestrators/steps": {
      "import": "./dist/src/orchestrators/steps/index.js",
      "require": "./dist/src/orchestrators/steps/index.js",
      "types": "./dist/src/orchestrators/steps/index.d.ts"
    },
    "./promptBuilder": {
      "import": "./dist/src/promptBuilder/index.js",
      "require": "./dist/src/promptBuilder/index.js",
      "types": "./dist/src/promptBuilder/index.d.ts"
    },
    "./providers": {
      "import": "./dist/src/providers/index.js",
      "require": "./dist/src/providers/index.js",
      "types": "./dist/src/providers/index.d.ts"
    },
    "./tools": {
      "import": "./dist/src/tools/index.js",
      "require": "./dist/src/tools/index.js",
      "types": "./dist/src/tools/index.d.ts"
    },
    "./mcp": {
      "import": "./dist/src/mcp/index.js",
      "require": "./dist/src/mcp/index.js",
      "types": "./dist/src/mcp/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json"
  },
  "keywords": [
    "agent",
    "ai",
    "framework"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "dotenv": "^17.2.3",
    "openai": "^4.104.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.0.0"
  }
}
