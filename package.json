{
  "name": "@ericnunes/frame-agent-sdk",
  "version": "0.0.7",
  "description": "Framework criador de agentes com motor baseado em grafos, módulos reutilizáveis e um orchestrator baseado em steps muito simples e fácil de configurar.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./agent": {
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.js",
      "types": "./dist/agent/index.d.ts"
    },
    "./agents": {
      "import": "./dist/agent/index.js",
      "require": "./dist/agent/index.js",
      "types": "./dist/agent/index.d.ts"
    },
    "./llmModes": {
      "import": "./dist/llmModes/index.js",
      "require": "./dist/llmModes/index.js",
      "types": "./dist/llmModes/index.d.ts"
    },
    "./llm": {
      "import": "./dist/llmModes/index.js",
      "require": "./dist/llmModes/index.js",
      "types": "./dist/llmModes/index.d.ts"
    },
    "./memory": {
      "import": "./dist/memory/index.js",
      "require": "./dist/memory/index.js",
      "types": "./dist/memory/index.d.ts"
    },
    "./orchestrators/graph": {
      "import": "./dist/orchestrators/graph/index.js",
      "require": "./dist/orchestrators/graph/index.js",
      "types": "./dist/orchestrators/graph/index.d.ts"
    },
    "./promptBuilder": {
      "import": "./dist/promptBuilder/index.js",
      "require": "./dist/promptBuilder/index.js",
      "types": "./dist/promptBuilder/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./tools": {
      "import": "./dist/tools/index.js",
      "require": "./dist/tools/index.js",
      "types": "./dist/tools/index.d.ts"
    },
    "./tools/mcp": {
      "import": "./dist/tools/tools/mcp/index.js",
      "require": "./dist/tools/tools/mcp/index.js",
      "types": "./dist/tools/tools/mcp/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "prepare": "npm run build",
    "clean:src": "bash scripts/clean-src-js.sh",
    "clean:all": "rm -rf dist/ && npm run clean:src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit"
  },
  "keywords": [
    "agent",
    "ai",
    "framework"
  ],
  "author": "Eric Nunes",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericnunes30/frame-agent-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ericnunes30/frame-agent-sdk/issues"
  },
  "homepage": "https://github.com/ericnunes30/frame-agent-sdk#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "dotenv": "^17.2.3",
    "fast-diff": "^1.3.0",
    "glob": "^13.0.0",
    "js-tiktoken": "^1.0.21",
    "openai": "^4.104.0",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.27",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.25.13",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.0.0"
  }
}
